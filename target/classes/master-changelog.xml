<databaseChangeLog
xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <!-- <preConditions onError="HALT" onFailMessage="Unauthorized access" onErrorMessage="custom error message">
        <dbms type="postgresql"/>
        <runningAs username="SYSTEM"/>
        <columnExists tableName="" columnName=""/>
        <rowCount expectedRows="0" tableName="" />
    </preConditions> -->

    <property name="table_name1" value="Emp"/>
    <property name="table_name2" value="Dept"/>

    <changeSet id="change_1" author="izdebski">
        <createTable tableName="${table_name1}">
            <column name="EmpId" type="int"></column>
            <column name="EmpName" type="varchar(10)"></column>
        </createTable>
    </changeSet>

    <changeSet id="change_2" author="izdebski">
            <createTable tableName="${table_name2}">
                <column name="DeptNo" type="int"></column>
                <column name="DeptName" type="varchar(10)"></column>
            </createTable>
    </changeSet>
</databaseChangeLog>